{
	"manifest_version": 2,
	"name": "Drupal PHP Input Improver",
	"version": "1.1.0",
	"description": "Adds CodeMirror to certain fields on a drupal site",
	"icons": {
		"16": "icons/16.png",
		"48": "icons/48.png",
		"128": "icons/128.png"
	},
	"content_scripts": [ {
		"exclude_globs": [ ],
		"include_globs": [ "*" ],
		"js": [ "drupal-devel-php-improver.user.js" ],
		"matches": [
			"http://*/devel/php",
			"https://*/devel/php",
			"http://*/admin/structure/ds/fields/manage_custom/*",
			"https://*/admin/structure/ds/fields/manage_custom/*"
		],
		"run_at": "document_end"
	} ],
	"converted_from_user_script": true
}
